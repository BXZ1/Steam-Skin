htmlfindbar.layout
{
	colors
	{
		HTML.SearchWide=320
		HTML.SearchTall=46
	}
	styles
	{
		html-findbar
		{
			minimum-height=46
			bgcolor="popover_bg"
			render
			{
				0="fill(x0, y0, x1, y0+1, border)"
				1="fill(x0, y1-1, x1, y1, border)"
				2="fill(x0, y0+1, x0+1, y1-1, border)"
				3="fill(x1-1, y0+1, x1, y1-1, border)"
			}
		}

		FindEntry
		{
			inset-left=28
			render
			{
				0="image(x0+8, y0+9, x0+24, y1-9, assets/icons/search_backdrop)"
			}
		}
		FindEntry:framefocus
		{
			render
			{
				0="image(x0+8, y0+9, x0+24, y1-9, assets/icons/search)"
			}
		}

		Label
		{
			textcolor="button_disabled_fg"
		}

		PrevButtonSm
		{
			inset-left=3
			image="assets/arrows/outline_w/up_backdrop"
			render_bg
			{
				0="fill(x0+6, y0, x1, y1, button_bg)"
				1="fill(x0, y0+6, x0+6, y1-6, button_bg)"
				2="image(x0, y0, x0+6, y0+6, assets/corners/6_w10/tl)"
				3="image(x0, y1-6, x0+6, y1, assets/corners/6_w10/bl)"
			}
			render
			{
				0="fill(x1, y0, x1+1, y1, border)"
			}
		}
		PrevButtonSm:framefocus
		{
			image="assets/arrows/outline_w/up"
		}
		PrevButtonSm:hover
		{
			render_bg
			{
				0="fill(x0+6, y0, x1, y1, button_hover_bg)"
				1="fill(x0, y0+6, x0+6, y1-6, button_hover_bg)"
				2="image(x0, y0, x0+6, y0+6, assets/corners/6_w15/tl)"
				3="image(x0, y1-6, x0+6, y1, assets/corners/6_w15/bl)"
			}
		}
		PrevButtonSm:active
		{
			render_bg
			{
				0="fill(x0+6, y0, x1, y1, button_active_bg)"
				1="fill(x0, y0+6, x0+6, y1-6, button_active_bg)"
				2="image(x0, y0, x0+6, y0+6, assets/corners/6_w30/tl)"
				3="image(x0, y1-6, x0+6, y1, assets/corners/6_w30/bl)"
			}
		}
		PrevButtonSm:disabled
		{
			image="assets/arrows/outline_w/up_backdrop"
			render_bg
			{
				0="fill(x0+6, y0, x1, y1, button_disabled_bg)"
				1="fill(x0, y0+6, x0+6, y1-6, button_disabled_bg)"
				2="image(x0, y0, x0+6, y0+6, assets/corners/6_w5/tl)"
				3="image(x0, y1-6, x0+6, y1, assets/corners/6_w5/bl)"
			}
		}

		NextButtonSm
		{
			inset-left=3
			image="assets/arrows/outline_w/down_backdrop"
			render_bg
			{
				0="fill(x0, y0, x1-6, y1, button_bg)"
				1="fill(x1-6, y0+6, x1, y1-6, button_bg)"
				2="image(x1-6, y0, x1, y0+6, assets/corners/6_w10/tr)"
				3="image(x1-6, y1-6, x1, y1, assets/corners/6_w10/br)"
			}
		}
		NextButtonSm:framefocus
		{
			image="assets/arrows/outline_w/down"
		}
		NextButtonSm:hover
		{
			render_bg
			{
				0="fill(x0, y0, x1-6, y1, button_hover_bg)"
				1="fill(x1-6, y0+6, x1, y1-6, button_hover_bg)"
				2="image(x1-6, y0, x1, y0+6, assets/corners/6_w15/tr)"
				3="image(x1-6, y1-6, x1, y1, assets/corners/6_w15/br)"
			}
		}
		NextButtonSm:active
		{
			render_bg
			{
				0="fill(x0, y0, x1-6, y1, button_active_bg)"
				1="fill(x1-6, y0+6, x1, y1-6, button_active_bg)"
				2="image(x1-6, y0, x1, y0+6, assets/corners/6_w30/tr)"
				3="image(x1-6, y1-6, x1, y1, assets/corners/6_w30/br)"
			}
		}
		NextButtonSm:disabled
		{
			image="assets/arrows/outline_w/down_backdrop"
			render_bg
			{
				0="fill(x0, y0, x1-6, y1, button_disabled_bg)"
				1="fill(x1-6, y0+6, x1, y1-6, button_disabled_bg)"
				2="image(x1-6, y0, x1, y0+6, assets/corners/6_w5/tr)"
				3="image(x1-6, y1-6, x1, y1, assets/corners/6_w5/br)"
			}
		}

		CloseButtonSm
		{
			minimum-width=24
			minimum-height=24
			image="assets/pixel"
			render_bg
			{
				0="image(x0, y0, x1, y1, assets/windowcontrols/bg_backdrop)"
				1="image(x0, y0, x1, y1, assets/windowcontrols/close_backdrop)"
			}
		}
		CloseButtonSm:disabled
		{
			render_bg
			{
				0="image(x0, y0, x1, y1, assets/windowcontrols/bg_backdrop)"
				1="image(x0, y0, x1, y1, assets/windowcontrols/close_backdrop)"
			}
		}
		CloseButtonSm:framefocus
		{
			render_bg
			{
				0="image(x0, y0, x1, y1, assets/windowcontrols/bg)"
				1="image(x0, y0, x1, y1, assets/windowcontrols/close)"
			}
		}
		CloseButtonSm:hover
		{
			render_bg
			{
				0="image(x0, y0, x1, y1, assets/windowcontrols/bg_hover)"
				1="image(x0, y0, x1, y1, assets/windowcontrols/close)"
			}
		}
		CloseButtonSm:active
		{
			render_bg
			{
				0="image(x0, y0, x1, y1, assets/windowcontrols/bg_active)"
				1="image(x0, y0, x1, y1, assets/windowcontrols/close)"
			}
		}
	}
	layout
	{
		place { control="FindEntry" margin-left=6 margin-top=6 width=220 height=34 }
		place { control="FindCount" align=right margin-right=128 margin-top=16 }
		place { control="Previous,Next" margin-left=232 margin-top=6 width=34 height=34 spacing=1 }
		place { control="Close" align=right margin-right=11 margin-top=11 width=24 height=24 }

		region { name="hidden" width=0 height=0 margin-left=-999 }
		place { control="SearchThisPage" region="hidden" width=0 height=0 }
	}
}