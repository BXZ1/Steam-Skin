gameoverlay.styles
{
	include "resource/styles/steam.styles"
	
	styles 
	{
		Frame
		{
			minimum-width=534
			render
			{
				0="fill(x0, y0, x1, y0+1, window_border)"
				1="fill(x0, y1-1, x1, y1, window_border)"
				2="fill(x0, y0+1, x0+1, y1-1, window_border)"
				3="fill(x1-1, y0+1, x1, y1-1, window_border)"
			}
		}

		FrameTitle
		{
			minimum-height=48
			textcolor=none
			font-size=1
		}

		Page
		{
			bgcolor=none
		}

		PageTab
		{
			minimum-height=34
		}
	}
}
